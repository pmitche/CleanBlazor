@page "/account"
@using Microsoft.Extensions.Localization
@inject IStringLocalizer<Account> Localizer

<HeroTitle Title="@Localizer["Account"]" Description="@Localizer["Manage your account"]"/>
<MudTabs Elevation="25" Color="Color.Transparent" Rounded="true" PanelClass="mt-6">
    <MudTabPanel Text="@Localizer["Profile"]">
        <Profile/>
    </MudTabPanel>
    <MudTabPanel Text="@Localizer["Security"]">
        <Security/>
    </MudTabPanel>
</MudTabs>
